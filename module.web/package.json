    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.2",
    "jest": "^27.2.0",
    "jest-cli": "^27.2.0",
    "puppeteer": "^10.0.0",
    "typescript": "^4.4.3"
